{
  "name": "Color Picker",
  "description": "Pick color from any webpage",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "./popup.html"
  },
  "icons": {
    "16": "/images/picker_16.png",
    "32": "/images/picker_32.png",
    "48": "/images/picker_48.png"
  }
}